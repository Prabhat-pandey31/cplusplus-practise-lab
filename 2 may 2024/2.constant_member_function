
#include <iostream>
using namespace std;

class example{
    int dataone;
    int datatwo;
    int datathree;
//                                              ----------------------- access specifiers ------------------------------------------------------
    public:
  //                           ------------- constructor ------------------------------
    example() 
    {   dataone = 10;
        datatwo = 40;
        datathree = 900;
        
    }
    void disp();
    void disp()const;
       
};

int main()
    
    const example villian;
    villian.disp();
}
//                                            ----------- member function ---------------------
void example:: disp(){
    cout<<dataone<<endl;
    cout<<datatwo<<endl;
    cout<<datathree<<endl;
}

void example:: disp()const{
    cout<<dataone<<endl;
    cout<<datatwo<<endl;
    cout<<datathree<<endl;
}
